{
  "name": "my-uniform-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "uniform:pull": "uniform context signal pull ./uniform/signals",
    "uniform:push": "uniform context signal push ./uniform/signals",
    "dev": "npm run download:manifest && next dev",
    "build": "npm run download:manifest && next build",
    "download:manifest": "uniform context manifest download --output ./contextManifest.json",
    "start": "next start"
  },
  "dependencies": {
    "@uniformdev/cli": "^16.2.0",
    "@uniformdev/context": "^16.2.0",
    "@uniformdev/context-react": "^16.2.0",
    "next": "12.1.0",
    "react": "17.0.1",
    "react-dom": "17.0.1"
  }
}
